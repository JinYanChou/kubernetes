<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mycat:rule SYSTEM "rule.dtd">
<mycat:rule xmlns:mycat="http://io.mycat/">

	<tableRule name="sharding-by-stringhash-dtabm001">
		<rule>
			<columns>APLY_NO,TRN_KIND,POLICY_NO</columns>
			<algorithm>sharding-by-stringhash</algorithm>
		</rule>
	</tableRule>

	<tableRule name="sharding-by-stringhash-dtab0007">
		<rule>
			<columns>POLICY_NO,PROD_ID,ID,SP_EFT_DATE</columns>
			<algorithm>sharding-by-stringhash</algorithm>
		</rule>
	</tableRule>

	<tableRule name="sharding-by-stringhash-dtaga505">
		<rule>
			<columns>PROD_ID,AGE,SEX,PAY_PRD,PREM_SPEC_KEY1,PREM_SPEC_KEY2,PASS_YEAR</columns>
			<algorithm>sharding-by-stringhash</algorithm>
		</rule>
	</tableRule>

	<function name="sharding-by-stringhash-dtabm001" class="io.mycat.route.function.PartitionByString">
		<property name="partitionLength">512</property>
		<property name="partitionCount">3</property>
		<property name="hashSlice">0:4</property>
	</function>

	<function name="sharding-by-stringhash-dtab0007" class="io.mycat.route.function.PartitionByString">
		<property name="partitionLength">512</property>
		<property name="partitionCount">3</property>
		<property name="hashSlice">0:4</property>
	</function>

	<function name="sharding-by-stringhash-dtaga505" class="io.mycat.route.function.PartitionByString">
		<property name="partitionLength">512</property>
		<property name="partitionCount">3</property>
		<property name="hashSlice">0:4</property>
	</function>

	<function name="sharding-by-stringhash" class="io.mycat.route.function.PartitionByString">
		<property name="partitionLength">512</property>
		<property name="partitionCount">2</property>
		<property name="hashSlice">0:4</property>
	</function>

</mycat:rule>
